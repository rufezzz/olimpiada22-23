https://codeforces.com/gym/103471/problem/2

#include <map>
#include <iostream>
int main( )
{
   using namespace std;
   int n, m;
   cin>>n>>m;
   
   
   std::string A, B;
   cin>>A>>B;
   
   
   std::map<std::pair<int, int>, int> map;
   
   int x = 0;
   int y = 0;
   
   map.insert(make_pair(make_pair(x,y), 1));
   for(int i = 0; i < (n+m); i++)
   {
       if(A[i] == 'R')
       {
           x = x + 1;
           map.insert(make_pair(make_pair(x,y), 1));
       }
       else
       {
           y = y + 1;
           map.insert(make_pair(make_pair(x,y), 1));
       }
    }
    int b = 0;
    int c = 0;
    for(int i = 0; i < (n+m); i++)
    {
        if(B[i] == 'R')
        {
            b = b + 1;
            map.insert(make_pair(make_pair(b,c), 1));
        }
        else
        {
            c = c + 1;
            map.insert(make_pair(make_pair(b,c), 1));
        }
    }
    int size = map.size();
    cout<<(n+m)*2 - size;
    return 0;
}
