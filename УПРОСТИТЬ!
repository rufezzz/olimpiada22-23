https://codeforces.com/gym/104010/problem/G

s = int(input())
lr = str("")
b = 0
for i in range(0, 10**18 + 1):
    lr = lr + str(i)
    b = b + 1
    if len(lr) >= s:
        break
print(b)
print(0, b-1)



using namespace std;

int main()
{
    int s;
    cin>>s;
    std:string lr = "";
    int b = 0;
    
    for (int i = 0; lr.size() < s; i++)
    {
        lr += std::to_string(i);
        b = b + 1;
    }
    
    cout<<b<<'\n'<<"0"<<" "<<b-1;
    

    return 0;
}
