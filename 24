https://codeforces.com/gym/103401/problem/F


#include <iostream>

using namespace std;

int main()
{
    int n, m, l, r;
    cin>>n>>m>>l>>r;
    
    int ras[n];
    for (int i = 0; i<n; i++)
    {
        cin>>ras[i];
    }
    
    int V = 0;
    int b = 0;
    int c = n;
    
    while (c > 0 && m > 0 && b != n)
    {
        
        if (l <= ras[b] && ras[b] <= r)
        {
            if(m>0)
            {
                m = m - 1;
                V = V + 1;
                c = c - 1;
            }
        }
        else
        {
            if (m > 1)
            {
                m = m - 2;
                V = V + 1;
                c = c - 1;
            }
        }
        b = b + 1;
    }
    if (V == n)
    {
        cout<<"lzdak!";
    }
    else
    {
        cout<<V;
    }
    return 0;
}
