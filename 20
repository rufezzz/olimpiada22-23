https://codeforces.com/gym/103566/problem/A



#include <iostream>

using namespace std;

int main()
{
    char s;
    cin>>s;
    
    if (s == 'A' || s == 'D' || s == 'O' || s == 'P' || s == 'Q' || s == 'R')
    {
        cout<<"1";
    }
    else if (s == 'B')
    {
        cout<<"2";
    }
    else
    {
        cout<<"0";
    }

    return 0;
}
